.container {
  width: 92%;
  margin: 0 auto;
}
.dayHoliday {
  width: 100%;
  height: 500rpx;
  box-sizing: border-box;
  border: 2rpx solid;
}
.content {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.holiday {
  color: white;
  flex: 0 1 23%;
  height: 160rpx;
  margin-top: 20rpx;
  line-height: 100rpx;
  text-indent: 24rpx;
  box-sizing: border-box;
  border-radius: 8rpx;
}
.holiday navigator {
  height: 100%;
}